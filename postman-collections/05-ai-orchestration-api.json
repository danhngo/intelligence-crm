              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"model_name\": \"lead_scoring_v3\",\n  \"training_data\": {\n    \"source\": \"database\",\n    \"query\": \"SELECT * FROM contact_features WHERE created_at > '2024-01-01'\",\n    \"target_column\": \"converted\"\n  },\n  \"algorithm\": \"xgboost\",\n  \"hyperparameters\": {\n    \"learning_rate\": 0.1,\n    \"max_depth\": 6,\n    \"n_estimators\": 200\n  },\n  \"validation\": {\n    \"method\": \"cross_validation\",\n    \"folds\": 5,\n    \"test_size\": 0.2\n  },\n  \"auto_deploy\": false\n}"
            },
            "url": {
