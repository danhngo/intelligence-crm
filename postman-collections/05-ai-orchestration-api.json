        {
          "name": "Deploy Model",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{auth_token}}",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"lead_scoring_model_v2\",\n  \"description\": \"Enhanced lead scoring model with improved accuracy\",\n  \"type\": \"classification\",\n  \"framework\": \"scikit-learn\",\n  \"version\": \"2.1.0\",\n  \"model_config\": {\n    \"algorithm\": \"random_forest\",\n    \"hyperparameters\": {\n      \"n_estimators\": 100,\n      \"max_depth\": 10,\n      \"random_state\": 42\n    }\n  },\n  \"input_schema\": {\n    \"type\": \"object\",\n    \"properties\": {\n      \"engagement_score\": {\"type\": \"number\"},\n      \"profile_score\": {\"type\": \"number\"},\n      \"interaction_frequency\": {\"type\": \"number\"}\n    },\n    \"required\": [\"engagement_score\", \"profile_score\"]\n  }\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/{{apiVersion}}/models",
              "host": ["{{baseUrl}}"],
              "path": ["api", "{{apiVersion}}", "models"]
            }
          },
          "response": []
        },
        {
          "name": "Predict with Model",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{auth_token}}",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"inputs\": {\n    \"engagement_score\": 0.75,\n    \"profile_score\": 0.85,\n    \"interaction_frequency\": 12\n  },\n  \"options\": {\n    \"include_confidence\": true,\n    \"include_explanation\": true\n  }\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/{{apiVersion}}/models/{{model_id}}/predict",
              "host": ["{{baseUrl}}"],
              "path": ["api", "{{apiVersion}}", "models", "{{model_id}}", "predict"]
